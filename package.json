{
  "name": "bignote",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildDev": "node-sass scss/master.sass public/css/master.css && browserify web/App.js -t [ babelify --presets [ latest react ] ] > public/js/app.js",
    "build": "npm install && node-sass scss/master.sass public/css/master.css && browserify web/App.js -t [ babelify --presets [ latest react ] ] -g [ envify --NODE_ENV production ] -g uglifyify | terser --compress --mangle > public/js/app.js",
    "startDev": "npm run buildDev && node server.js",
    "start": "npm run build && node server.js",
    "forever": "npm run build && pm2 start ecosystem.config.js --only bignote",
    "reload": "npm run build && pm2 reload bignote"
  },
  "author": "",
  "license": "ISC",
